{
  "name": "streetlives-api",
  "version": "0.1.0",
  "description": "The Streetlives backend providing the API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "8.2.0",
    "npm": "5.3.0"
  },
  "keywords": [
    "streetlives"
  ],
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.14.8"
  }
}
